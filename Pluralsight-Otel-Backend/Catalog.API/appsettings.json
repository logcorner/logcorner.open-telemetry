{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",
  "CORS": {
    "main": [
      "http://localhost:4200"
    ]
  },
  "Otlp": {
    "ServiceName": "Catalog.API",
    "Endpoint": "http://localhost:4317"
  },
  "Jaeger": {
    "Hostname": "localhost",
    "PortNumber": 6831
  },
  "Zipkin": {
    "Hostname": "localhost",
    "PortNumber": 9412
  }
}